<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- favicon -->
    <link rel="apple-touch-icon" sizes="57x57" href="{{ filePath }}assets/new-favicon/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ filePath }}assets/new-favicon//apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ filePath }}assets/new-favicon//apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ filePath }}assets/new-favicon//apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ filePath }}assets/new-favicon//apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ filePath }}assets/new-favicon//apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ filePath }}assets/new-favicon//apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ filePath }}assets/new-favicon//apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ filePath }}assets/new-favicon//apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ filePath }}assets/new-favicon//android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ filePath }}assets/new-favicon//favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ filePath }}assets/new-favicon//favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ filePath }}assets/new-favicon//favicon-16x16.png">
    <link rel="manifest" href="{{ filePath }}assets/new-favicon//manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{ filePath }}assets/new-favicon//ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
    <title>{{ pageTitle }}</title>
    <!-- third party CDNs -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <!-- emailjs -->
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@3/dist/email.min.js"></script>
    <link rel="stylesheet" href="{{ filePath }}css/styles.css">
    <link id="theme-style" rel="stylesheet" href="{{ filePath }}css/light.css">
</head>
<body>
    {% include 'includes/skip-link.njk' %}
    <main>
        {% include 'includes/nav.njk' %}
        {% if pageTitle === 'Home | Amper Digital' %}
          {% include 'includes/index-hero.njk' %}
        {% endif %}
        <div class="container" id="content">
          {% block content %}
          {% endblock %}
        </div>
        {% include 'includes/footer.njk' %}
    </main>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="{{ filePath }}js/main.js"></script>
    <script src="{{ filePath }}js/email.js"></script>
</body>
</html>